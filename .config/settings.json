        {
            "command": {
                "action": "splitPane",
                "index": 0,
                "split": "auto"
            },
            "keys": [
                "alt+shift+l"
            ]
        },
        {
            "command": "find",
            "keys": [
                "ctrl+f"
            ]
        },
        {
            "command": "togglePaneZoom",
